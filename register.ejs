<%include header.ejs%>
<div id="wrapper">
        <%include logo.ejs%>
            <%include nav.ejs%>
                <%include slider.ejs%>
                    <div id="page">
                        <div id="content">
                            <div class="box">
                                <h2>User Registration...</h2>
                                <br><br>
                               <h2 style="color: green"><%= result%></h2>
                                <form action="/register" method="POST">
                                    <table cellspacing="20">
                                        <tr>
                                            <td>
                                                Name
                                            </td>
                                            <td>
                                                <input type="text" name="nm" />
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                Username
                                            </td>
                                            <td>
                                                <input type="email" name="unm" placeholder="Enter your email" />
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                Password
                                            </td>
                                            <td>
                                                <input type="password" name="pass" />
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>Address</td>
                                            <td>
                                                <textarea name="address"></textarea>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                City
                                            </td>
                                            <td>
                                                <select name="city">
                                                    <option>Select</option>
                                                    <option>Indore</option>
                                                    <option>Bhopal</option>
                                                    <option>delhi</option>
                                                </select>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                Mobile No.
                                            </td>
                                            <td>
                                                <input type="text" name="mob" />
                                            </td>
                                        </tr>
                                        <tr>
                                            <td>
                                                Gender
                                            </td>
                                            <td>
                                                <input type="radio" name="gender" value="Male"/>Male&nbsp;&nbsp;
                                                <input type="radio" name="gender" value="female"/>Female</td>
                                        </tr>
                                        <tr>
                                            <td></td>
                                            <td>
                                                <input type="submit" name="s" value="Register" />
                                            </td>
                                        </tr>
                                      
                                    </table>
                                </form>
                            </div>
                        </div>
                        <div id="sidebar">
                            <div class="box">
                                <h3>Login with</h3>
                                <ul class="list">
                                    <li class="first">
                                        <a href="#">Facebook</a>
                                    </li>
                                    <li>
                                        <a href="#">Google</a>
                                    </li>
                                    <li>
                                        <a href="#">Twitter</a>
                                    </li>
                                    <li>
                                        <a href="#">LinkedIN</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <br class="clearfix"
                    </div>
                    <div id="page-bottom">
                        <div id="page-bottom-content">
                            <h3>Product Info</h3>
                            <p>
                                Product certification or product qualification is the process of certifying that a certain product has passed performance
                                tests and quality assurance tests, and meets qualification criteria stipulated in contracts,
                                regulations, or specifications (typically called "certification schemes" in the product certification
                                ...
                            </p>
                        </div>
                        <br class="clearfix" />
                    </div>
</div>
<%include footer.ejs%>